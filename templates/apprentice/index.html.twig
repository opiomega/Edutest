						<div
							class="courses-section">
							{# card header #}
							<div class="card-header">

								<div class="container">
									<div class="row">
										<div class="col-7">
											<p class="card-title">My college list
											</p>
										</div>
										<div class="col">
											<button class="btn-see-all">see all
											</button>
										</div>
									</div>

								</div>
								<div class="container">
									<div class="row">
										<div class="col">
											<img src="{{asset('image/college.png')}}" class="college-pic">
										</div>
									</div>
								</div>


							</div>
							{# end card header #}

							{# start card content #}
							<div class="card-content">

								<div class="college-lists">
									<button class="show">show</button>
									<img src="">
									<span>college 1
										<br>
										deadline</span>


								</div>
								<div class="college-lists">
									<button class="show">show</button>
									<img src="">
									<span>college 2
										<br>
										deadline</span>


								</div>

							</div>
							{# end card content #}

						</div>


					</div>
					{# end college card #}


					{# start of club card #}
					<div class="col ">
						<div
							class="courses-section">
							{# card header #}
							<div class="card-header">

								<div class="container">
									<div class="row">
										<div class="col-7">
											<p class="card-title">My clubs
											</p>
										</div>
										<div class="col">
											<button class="btn-see-all">see all
											</button>
										</div>
									</div>

								</div>
								<div class="container">
									<div class="row">
										<div class="col d-flex justify-content-center">
											<img src="{{asset('image/club-pic.png')}}" class="club-pic">
										</div>
									</div>
								</div>

							</div>
							{# end card header #}

							{# start card content #}

							<div class="card-content">

								<div class="college-lists">
									<button class="show">show</button>
									<img src="">

									<span>Debate
										<br>deadline 15/02/1225</span>


								</div>
								<div class="college-lists">
									<button class="show">show</button>
									<img src="">

									<span>Debate
										<br>deadline 15/02/2555</span>


								</div>

							</div>
							{# end card content #}
						</div>


					</div>
					{# end club card #}


					{# start of documents card #}
					<div class="col ">
						<div
							class="courses-section">
							{# card header #}
							<div class="card-header">

								<div class="container">
									<div class="row">
										<div class="col-7">
											<p class="card-title">Translated documents
											</p>
										</div>
										<div class="col">
											<button class="btn-see-all">see all
											</button>
										</div>
									</div>

								</div>
								<div class="container">
									<div class="row">
										<div class="col d-flex justify-content-center">
											<img src="{{asset('image/document-pic.png')}}" class="document-pic">
										</div>
									</div>
								</div>

							</div>
							{# end card header #}

							{# start card content #}

							<div class="card-content">

								<div class="row">

									<div class="col">


										<p class="Document-name">
											Document 1</p>
									</div>
									<div class="col-xs"></div>
									<div class="col">

										<button class="show">edit</button>

									</div>
								</div>
								<div class="row">

									<div class="col">


										<p class="Document-name">
											Document 2</p>
									</div>
									<div class="col-xs"></div>
									<div class="col">

										<button class="show">edit</button>

									</div>
								</div>

							</div>
							{# end card content #}

						</div>


					</div>
					{# end documents card #}


				</div>

			</div>

		</section>


