{% extends 'base.html.twig' %}
{% block title %}Dashboard!
{% endblock %}
{% block stylesheets %}
	{{ parent() }}
{% endblock %}
{% block body %}
	<div class="container">
		<div class="row justify-content-center">
			<div class-"col-xl-12">
				<h1 class="title">Dashboard
				</h1>
			</div>
		</div>
	</div>


	<div class="payed">

		<div class="container-fluid no-gutters ">

			<div class="row ml-5 ">
				<div class="col-xl-7 ml-5 pb-5">
					<section class="application-progress-steps">


						<div class="row">
							<div class="col">
								<h3 class="design-process-title">Application Progress
								</h3>
							</div>
						</div>


						<div class="row">

							<div id="steps">
								<div class="step" data-desc="part 1">1</div>

								<div class="step " data-desc="Part 2">2</div>
								<div class="step active " data-desc="part 3">3</div>
								<div class="step " data-desc="Part 4">4</div>

							</div>

						</div>


					</section>

				</div>

				<div class="col-xl ml-0 mb-5">
					<div class="counseling">
						<div class="container">
							<div class="row">

								<div class="col-7">
									<p class="counseling-session-title">
										Counseling Session</p>
								</div>

								<div class="col">
									<button class="btn-see-all">see all
									</button>
								</div>
							</div>
							<br>
							<div class="row">
								<div class="col-l">
									<p>
										Counselor 1
									</p>
									<span>
										Topic</span>
								</div>
								<div class="col"></div>
								<div class="col">
									<br>
									<span>
										15/04/2020
									</span>
								</div>

							</div>
							<hr>

							<div class="row">
								<div class="col-l">
									<p>
										Counselor 2
									</p>
									<span>
										Topic</span>
								</div>
								<div class="col"></div>
								<div class="col">
									<br>
									<span>
										25/07/2021
									</span>
								</div>

							</div>


						</div>

					</div>


				</div>

			</div>


		</div>


		<section>
			<div class="container-fluid ">

				<div
					class="row ml-5">

					{# start of course card #}
					<div class="col ">
						<div
							class="courses-section">
							{# card header #}
							<div class="card-header">

								<div class="container">
									<div class="row">
										<div class="col-7">
											<p class="card-title">My courses
											</p>
										</div>
										<div class="col">
											<button class="btn-see-all">see all
											</button>
										</div>
									</div>

								</div>
								<div class="container">
									<div class="row">
										<div class="col d-flex justify-content-center">
											<img src="{{asset('image/mycourses.png')}}" class="course-pic">
										</div>
									</div>
								</div>

							</div>
							{# end card header #}

							{# start card content #}

							<div class="card-content">

								<div class="row">
									<div class="col">
										<p class="course-title">
											SAT math
										</p>
									</div>
								</div>

								<div class="row">
									<div class="col">
										<p class="course-lvl">
											Level 1
										</p>
									</div>
								</div>


								<div class="row">
									<div class="col">
										<div class="progress">
											<div class="progress-bar" role="progressbar" style="width: 25%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
										</div>
									</div>
								</div>


								<div class="row">
									<div class="col">
										<p class="courses-progress">
											Completed: 80%
										</p>
									</div>
								</div>

								<hr>
								<div class="row">
									<div class="col">
										<p class="title-next-session">

											Next Session
										</p>
									</div>
								</div>


								<div class="row">
									<div class="col">
										<p class="course-date">
											17/02/2020
										</p>
									</div>
								</div>


							</div>
							{# end card content #}
							<div class="row">
								<div class="col  d-flex justify-content-center">

									<div>
										<div class="dot active"></div>
										<div class="dot"></div>
										<div class="dot"></div>
										<div class="dot"></div>

									</div>

								</div>

							</div>
						</div>


					</div>
					{# end course card #}


					{# start of college card #}
