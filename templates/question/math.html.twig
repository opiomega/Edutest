<style>

ul li{
  color: #AAAAAA;
  display: block;
  position: relative;
  float: left;
  width: 100%;
  height: 60px;
	border-bottom: 1px solid #AAA;
}

ul li input[type=radio]{
  position: absolute;
  visibility: hidden;
}

ul li label{
  display: block;
  position: relative;
  font-weight: 200;
  font-size: 15px;
  padding: 13px 25px 25px 55px;
  margin: 10px auto;
  height: 30px;
  z-index: 9;
  cursor: pointer;
  -webkit-transition: all 0.25s linear;
}

ul li:hover label{
	/*color: #FFFFFF;*/
}

ul li .check{
  display: block;
  position: absolute;
  border: 2px solid #B2273D;
  border-radius: 100%;
  height: 20px;
  width: 20px;
  top: 20px;
  left: 20px;
	z-index: 5;
	transition: border .25s linear;
	-webkit-transition: border .25s linear;
}

ul li:hover .check {
  border: 5px solid #FFFFFF;
}

ul li .check::before {
  display: block;
  position: absolute;
	content: '';
  border-radius: 100%;
  height: 10px;
  width: 10px;
  top: 3px;
	left: 3px;
  margin: auto;
	transition: background 0.25s linear;
	-webkit-transition: background 0.25s linear;
}

input[type=radio]:checked ~ .check {
  border: 2px solid rgb(0, 132, 206);
}

input[type=radio]:checked ~ .check::before{
  background: #B2273D;
}

input[type=radio]:checked ~ label{
  color: #394f79 !important;
  font-weight:bold;
}

</style>
