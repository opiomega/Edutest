<form method="post" action="{{ path('educationlevel_delete', {'id': educationlevel.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ educationlevel.id) }}">
    <button class="btn btn-success">Delete</button>
</form>
